<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceBlock name="contactForm">
            <action method="setTemplate" ifconfig="desk_general/default_settings/contact_page_visibility">
                <argument name="template" xsi:type="string">Cart2Quote_Desk::magento/contact/form.phtml</argument>
            </action>
            <block class="Cart2Quote\Desk\Block\Magento\Contact\Success" name="ticket.message.success" template="Cart2Quote_Desk::magento/contact/success.phtml" cacheable="false" ifconfig="desk_general/default_settings/contact_page_visibility"/>
        </referenceBlock>
        <referenceContainer name="sidebar.main">
            <block class="Infortis\UltraMegamenu\Block\Navigation"
                   name="umm.sidemenu.catalog_category_view.main"
                   as="umm.sidemenu.catalog_category_view.main"
                   template="Infortis_UltraMegamenu::categories.phtml"
                   before="-"
                   ifconfig="ultramegamenu/sidemenu/enable_category_main"
                   />
        </referenceContainer>
    </body>
</page>
